<div class="containter">
  <div class="list-header">
    <h3>Review your new shopping list!</h3>
  </div>
  <div class="shopping-list">
    <h4><%= @last_date %> - <%= @today %></h4>
    <ul>
      <% @items.each do |item| %>
        <li class="item"><%= item[0] %>
        <% if !item[1].nil? %>
          <a href="<%= item[1] %>"><%= item[1] %></a></li>
        <% end %>
      <% end %>
      </ul>
  </div>
</div>
